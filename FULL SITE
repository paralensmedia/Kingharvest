	});	
	function scrollWin()
	{    window.scrollTo(0, 0);		}
	function cleanUp()
	{
		$(".cross").hide();
		$(".menu").hide();
		$(".hamburger").css({ "display" : "block" });
		$(".second-level-menu-mobile").css({ "display" : "none" });
