var noop = 0;
function mt()
{
	var ip_add = "99.108.9.8";
	var brow = "Mozilla/5.0 (Macintosh; Intel Mac OS X 10_10_5) AppleWebKit/603.2.5 (KHTML, like Gecko) Version/10.1.1 Safari/603.2.5";
	var page = "";
	var referer = document.referrer;
  $.ajax
   ({
				method:'POST',
				url:'https://kingharvest.org/mt.php',
				data:'ip=' + ip_add + '&brow=' + brow + '&ref=' + referer + '&page=/PhoneClick' + page
		});
}
</script>
