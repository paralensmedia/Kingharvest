  var ckt = $.cookie('_kh', Number);
  if (ckt == null)     {   $("#overlay_0").show();	 }
  else                    {   $("#overlay_0").hide();    }
  $("#isOfAge").click(function()
  {
      $.cookie("_kh", '20181228', { expires: 30, path: '/' });
      $("#overlay_0").css({"display" : "none"});
  });

	$(window).scroll(function()
	{
		if ($(window).scrollTop() > 25)
		{
			$(".top-level-menu").css({ "margin-top" : "-10px", "height" : "40px" });
			$(".second-level-menu").css({ "background" : "rgba(80, 0, 0, .95)" });
			$("#tiny_logo").css({"height" : "35px"});
			$("#tiny_txt").css({"font-size" : "1.0em"});
			$("#sticker").css({"border-top" : "0px solid #000" });
			$("#sticker").css({"box-shadow" : "2px 2px 15px 3px #000" });
			$("#sticker").css({"border-bottom-right-radius" : "15px" });
			$("#sticker").css({"border-bottom-left-radius" : "15px" });
			$("#sticker").css({"background-color" : "#500" });
			$("#mhdr").css({"box-shadow" : "2px 2px 10px 3px #000" });
			$("#mhdr").css({"border-bottom-right-radius" : "10px" });
			$("#mhdr").css({"border-bottom-left-radius" : "10px" });
			$("#mhdr").css({"background-color" : "#500" });
		}
		else
		{
			$(".top-level-menu").css({ "margin-top" : "0px", "height" : "50px" });
			$(".second-level-menu").css({ "background-color" : "rgba(80, 0, 0, .95)" });
			$("#tiny_logo").css({"height" : "45px"});
